<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.ayselatak.firstkekodproject.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_egoFragment"
            app:destination="@id/egoFragment" />
        <action
            android:id="@+id/action_homeFragment_to_givingFragment"
            app:destination="@id/givingFragment" />
        <action
            android:id="@+id/action_homeFragment_to_happinessFragment"
            app:destination="@id/happinessFragment" />
        <action
            android:id="@+id/action_homeFragment_to_kindnessFragment"
            app:destination="@id/kindnessFragment" />
        <action
            android:id="@+id/action_homeFragment_to_optimismFragment"
            app:destination="@id/optimismFragment" />
        <action
            android:id="@+id/action_homeFragment_to_respectFragment"
            app:destination="@id/respectFragment" />
    </fragment>
    <fragment
        android:id="@+id/egoFragment"
        android:name="com.ayselatak.firstkekodproject.EgoFragment"
        android:label="fragment_ego"
        tools:layout="@layout/fragment_ego" />
    <fragment
        android:id="@+id/givingFragment"
        android:name="com.ayselatak.firstkekodproject.GivingFragment"
        android:label="fragment_giving"
        tools:layout="@layout/fragment_giving" />
    <fragment
        android:id="@+id/happinessFragment"
        android:name="com.ayselatak.firstkekodproject.HappinessFragment"
        android:label="fragment_happiness"
        tools:layout="@layout/fragment_happiness" />
    <fragment
        android:id="@+id/kindnessFragment"
        android:name="com.ayselatak.firstkekodproject.KindnessFragment"
        android:label="fragment_kindness"
        tools:layout="@layout/fragment_kindness" />
    <fragment
        android:id="@+id/optimismFragment"
        android:name="com.ayselatak.firstkekodproject.OptimismFragment"
        android:label="fragment_optimism"
        tools:layout="@layout/fragment_optimism" />
    <fragment
        android:id="@+id/respectFragment"
        android:name="com.ayselatak.firstkekodproject.RespectFragment"
        android:label="fragment_respect"
        tools:layout="@layout/fragment_respect" />
</navigation>